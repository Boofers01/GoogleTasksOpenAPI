{
  "openapi": "3.1.0",
  "info": {
    "title": "Google Tasks API",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://tasks.googleapis.com"
    }
  ],
  "paths": {
    "/users/@me/lists": {
      "get": {
        "operationId": "listTaskLists",
        "summary": "List task lists",
        "responses": {
          "200": {
            "description": "A list of task lists"
          }
        }
      }
    },
    "/lists/{tasklist}/tasks": {
      "get": {
        "operationId": "listTasks",
        "summary": "List tasks in a task list",
        "parameters": [
          {
            "name": "tasklist",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "A list of tasks"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {}
  }
}
